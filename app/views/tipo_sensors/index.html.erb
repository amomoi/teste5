<p id="notice"><%= notice %></p>

<h1>Tipo Sensors</h1>

<table>
  <thead>
    <tr>
      <th>Tipo sensor</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @tipo_sensors.each do |tipo_sensor| %>
      <tr>
        <td><%= tipo_sensor.tipo_sensor %></td>
        <td><%= link_to 'Show', tipo_sensor %></td>
        <td><%= link_to 'Edit', edit_tipo_sensor_path(tipo_sensor) %></td>
        <td><%= link_to 'Destroy', tipo_sensor, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Tipo Sensor', new_tipo_sensor_path %>
